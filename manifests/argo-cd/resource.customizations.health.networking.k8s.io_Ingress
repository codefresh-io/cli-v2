hs = {}
type = obj.metadata.annotations["nginx.org/mergeable-ingress-type"]
if type == "minion" or type == "master" then
  hs.status = "Healthy"
  return hs
end
if obj.status.loadBalancer.ingress ~= nil then
  if table.getn(obj.status.loadBalancer.ingress) > 0 then
    hs.status = "Healthy"
    return hs
